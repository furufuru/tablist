{  
  "name": "TabList",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Import & Export tabs",
  "homepage_url": "https://github.com/furufuru/tablist",
  "icons": {
    "128": "icon128.png" 
  },
  "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
  "permissions": [
      "tabs"
  ],
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "TabList",
    "default_popup": "popup.html"
  }
}